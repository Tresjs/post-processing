    "release-it": "^15.11.0",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-visualizer": "^5.9.2",
    "typescript": "^5.1.3",
    "unocss": "^0.53.1",
    "vite": "^4.3.9",
    "vite-plugin-banner": "^0.7.0",
    "vite-plugin-dts": "2.3.0",
