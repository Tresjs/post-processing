    "rollup-plugin-visualizer": "^5.12.0",
    "three": "^0.167.0",
    "typescript": "^5.5.4",
    "unocss": "^0.61.5",
    "vite": "^5.3.5",
    "vite-plugin-banner": "^0.7.1",
    "vite-plugin-dts": "4.0.0-beta.1",
    "vite-svg-loader": "^5.1.0",
