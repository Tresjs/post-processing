{
  "name": "@tresjs/post-processing",
  "type": "module",
  "version": "1.0.0",
  "packageManager": "pnpm@9.14.2",
  "description": "Post-processing library for TresJS",
  "author": "Alvaro Saburido <hola@alvarosaburido.dev> (https://github.com/alvarosabu/)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tresjs/post-processing.git"
  },
  "keywords": [
    "vue",
    "3d",
    "threejs",
    "three",
    "post-processing",
    "effects",
    "fx"
  ],
  "maintainers": [
    "Alvaro Saburido (https://github.com/alvarosabu/)",
    "Tino Koch (https://github.com/Tinoooo)"
  ],
  "exports": {
    "./three": {
      "types": "./dist/three.d.ts",
      "import": "./dist/three.js"
    },
    "./pmndrs": {
      "types": "./dist/pmndrs.d.ts",
      "import": "./dist/pmndrs.js"
    },
    "./*": "./*"
  },
  "typesVersions": {
    "*": {
      "three": [
        "./dist/three.d.ts"
      ],
      "pmndrs": [
        "./dist/pmndrs.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "cd playground && npm run dev",
    "playground": "cd playground && npm run dev",
    "build": "vite build",
    "preview": "vite preview",
    "release": "release-it",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "test:attw": "attw --pack --ignore-rules cjs-resolves-to-esm"
  },
  "peerDependencies": {
    "@tresjs/core": ">=4.0",
    "three": ">=0.169",
    "vue": ">=3.4"
  },
  "dependencies": {
    "@vueuse/core": "^11.3.0",
    "postprocessing": "^6.36.4"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.17.2",
    "@release-it/conventional-changelog": "^9.0.3",
    "@tresjs/core": "^4.3.1",
    "@tresjs/eslint-config": "^1.4.0",
    "@types/three": "^0.170.0",
    "@unocss/core": "^0.64.1",
    "@vitejs/plugin-vue": "^5.2.1",
    "eslint": "^9.15.0",
    "gsap": "^3.12.5",
    "kolorist": "^1.8.0",
    "pathe": "^1.1.2",
    "release-it": "^17.10.0",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "three": "^0.170.0",
    "typescript": "5.7.2",
    "unocss": "^0.64.1",
    "vite": "^6.0.0",
    "vite-plugin-banner": "^0.8.0",
    "vite-plugin-dts": "4.3.0",
    "vite-svg-loader": "^5.1.0",
    "vitepress": "1.5.0",
    "vue": "^3.5.13",
    "vue-tsc": "^2.1.10"
  },
  "pnpm": {
    "overrides": {
      "typescript": "5.7.2"
    }
  }
}
